{
  "name": "turborepo-vite-shadcn-ui",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "volta:install": "curl https://get.volta.sh | bash",
    "ui": "pnpm --filter @repo/ui ui",
    "admin": "pnpm --filter super-admin dev",
    
    "staff":"pnpm --filter vector-staff dev",
    "build:admin": "pnpm --filter super-admin build",
    "build:staff": "pnpm --filter staff build",
    "deploy:admin": "turbo prune super-admin --docker && cd out && pnpm install --frozen-lockfile && pnpm build",
    "deploy:staff": "turbo prune staff --docker && cd out && pnpm install --frozen-lockfile && pnpm build"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/leaflet": "^1.9.20",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.1",
    "@syncfusion/ej2-react-barcode-generator": "^31.2.2",
    "@tanstack/react-table": "^8.21.3",
    "date-fns": "^4.1.0",
    "jsbarcode": "^3.12.1",
    "leaflet": "^1.9.4",
    "lucide-react": "0.263.1",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-quill-new": "^3.6.0",
    "sonner": "^2.0.7",
    "xlsx": "^0.18.5",
    "zod": "^4.0.15",
    "zustand": "^5.0.7"
  }
}
